{
    "plugins": [
        [
            "module-resolver",
            {
                "alias": {
                    "extensions": [".tsx", ".ts"]
                }
            }
        ]
    ],
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "current"
                }
            }
        ],
        "@babel/preset-react",
        [
            "@babel/preset-typescript",
            {
                "isTSX": true,
                "allExtensions": true
            }
        ]
    ]
}
