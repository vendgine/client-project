{
    "name": "@vngn/client-starter",
    "version": "0.0.1",
    "description": "Vendgine Client Starter package",
    "type": "module",
    "target": "web",
    "scripts": {
        "build": "rimraf dist && npm run --prefix node_modules/@vngn/client build",
        "build:production": "rimraf dist && npm run --prefix node_modules/@vngn/client build:production",
        "serve": "tsc -noEmit && npm run --prefix node_modules/@vngn/client serve",
        "prettier": "prettier \"**/*.{yml,yaml,json,md}\" --write",
        "lint": "eslint --fix --max-warnings 0 \"**/*.{mts,ts}\"",
        "test": "npm run --prefix node_modules/@vngn/client test",
        "prepare": "husky install",
        "pre-commit": "npm run prettier && npm run lint && npm run build"
    },
    "author": "Andrei Piontkovskii",
    "license": "SEE LICENSE IN LICENSE.txt",
    "devDependencies": {
        "@types/chai": "^4.3.3",
        "@types/common-tags": "^1.8.1",
        "@types/mocha": "^10.0.0",
        "@types/node": "^18.11.5",
        "@types/sinon": "^10.0.13",
        "@typescript-eslint/eslint-plugin": "^5.41.0",
        "@typescript-eslint/parser": "^5.41.0",
        "chai": "^4.3.6",
        "eslint": "^8.26.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsdoc": "^39.3.25",
        "eslint-plugin-simple-import-sort": "^8.0.0",
        "husky": "^8.0.1",
        "mocha": "^10.1.0",
        "prettier": "^2.7.1",
        "sinon": "^14.0.1",
        "tslib": "^2.4.0",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "@vngn/codestyle": "^0.0.2",
        "ajv": "^8.11.0",
        "chalk": "^5.1.2",
        "common-tags": "^1.8.2",
        "openapi3-ts": "^3.1.1",
        "schema-utils": "^4.0.0",
        "ts-essentials": "^9.3.0",
        "tsyringe": "^4.7.0"
    },
    "optionalDependencies": {
        "bufferutil": "^4.0.6",
        "utf-8-validate": "^5.0.9"
    }
}
